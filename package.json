    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "i18next": "^22.4.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "serverless-http": "^3.0.0",
    "stripe": "^12.15.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "ngrok": "^5.0.0-beta.2",
    "vite": "^4.4.9"
  }
}
